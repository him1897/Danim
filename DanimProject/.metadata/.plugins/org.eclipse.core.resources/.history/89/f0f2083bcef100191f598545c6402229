package com.kh.danim;

import java.util.Scanner;

public class test {
	public static void main(String[] args) {
	Scanner scan = new Scanner(System.in);
	
	System.out.print("배열의 갯수 입력 : ");
	int n = scan.nextInt();
	
	int A[] = new int [n];
	
	for(int i=0; i<A.length; i++) {
		System.out.print(i+1 + "번째 배열에 넣을 값 : ");
		A[i] = scan.nextInt();
	}
	
	}
}
